{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/client/static/$1"
    },
    {
      "src": "/.*\\.(js|json)",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/client/$0"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}
